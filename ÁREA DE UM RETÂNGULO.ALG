algoritmo "ÁREA DE UM RETÂNGULO"
var
   base: real
   altura: real
   area: real

inicio

   escreval ("Digite o valor da base do seu retângulo: ")
   leia (base)
   escreval ("Digite o valor da altura do seu retângulo: ")
   leia (altura)
   area <- base * altura
   escreva ("A área do seu retângulo é: ",area,"m²")


fimalgoritmo