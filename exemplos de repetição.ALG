algoritmo "semnome"
var
   contador: inteiro

inicio
// WHILE
   enquanto (contador <= 10) faca
      escreval("Funcionou a repetição!")
      contador <- contador + 1
   fimenquanto
   
// DO UNTIL
repita
   escreval("Funcionou a repetição!")
   contador <- contador + 1
   ate (contador <=10)
   
// FOR
para contador <- 1 ate 10 faca
  escreval("Funcionou a repetição!")
fimpara

fimalgoritmo